{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    ".idea"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": ""
  },
  "watch": [
    "routes/*",
    "bin/*",
    "app.js",
    "util/*"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js"
}
